{
  "name": "nestjs-pos-digicert",
  "version": "1.0.0",
  "description": "NestJS module for DigiCert digital signing integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "test": "jest",
    "prepare": "npm run build"
  },
  "keywords": [
    "nestjs",
    "digicert",
    "digital-signing",
    "sftp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "10.4.6",
    "@nestjs/core": "10.4.6",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@nestjs/common": "10.4.6",
    "@nestjs/core": "10.4.6",
    "@nestjs/testing": "10.4.6",
    "@types/jest": "29.5.14",
    "@types/node": "22.8.5",
    "@types/ssh2-sftp-client": "^9.0.4",
    "eslint": "^9.21.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.3.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "typescript-eslint": "8.26.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "soap": "1.1.9",
    "ssh2-sftp-client": "^11.0.0"
  }
}
